{
  "name": "simplePK",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: s-crud-ops",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
